function formatarCidade() {
  var city = document.querySelector('input[id="cidade"]').value;

  // Remove espa√ßos em branco
  var cidadeFormatada = city.replace(/\s/g, '');

  return cidadeFormatada;
}



function () {
  var city = document.querySelector('input[name="cidade"]').value;
  return city;
}
