function () {
  var primeiro = document.querySelectorAll('[id="variavel"]')[0].value; // Substitua pela vari치vel do seu formul치rio
  var segundo = document.querySelectorAll('[id="variavel"]')[1].value; // Substitua pela vari치vel do seu formul치rio
  var email;

  if (primeiro) {
    email = primeiro;
  } else {
    email = segundo;
  }

  // Configurando o cookie
  var expiryDate = new Date();
  expiryDate.setDate(expiryDate.getDate() + 30); // Define a validade do cookie para 30 dias
  document.cookie = "LeadEmail=" + encodeURIComponent(email) + "; expires=" + expiryDate.toUTCString() + "; path=/";

  return email;
}
