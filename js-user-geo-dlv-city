function formatCity() {
  var cityElement = {{dlv-city}}; // Substitua pela variável do seu formulário

  if (cityElement) {
    var cityValue = cityElement.value;

    // Converte para letras minúsculas e remove espaços
    var parsed = cityValue.toLowerCase().replace(/\s/g, '');

    return parsed;
  }

  return null; // Retorna null se o elemento não for encontrado
}
