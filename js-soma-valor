function() {
  var products = {{dlv at√© a matriz de dados}};
  var tam = products.length
  var TotalValue=0
  var indice=0
  while (tam !=0){
        TotalValue=TotalValue+(products[indice].price*products[indice].quantity)
        var tam=tam-1
        var indice=indice+1
      }  
  return TotalValue;
}
