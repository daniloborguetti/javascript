function formatCity() {
  // Substitua pela variável do seu formulário
  var city = document.querySelector('[id="city"]');

  // Verifica se city é um objeto válido e contém um valor
  if (typeof city === 'object' && city !== null && city.value.trim() !== '') {
    
    // Converte para letras minúsculas e remove espaços
    var cityValue = city.value.toLowerCase().replace(/\s/g, '');

    return cityValue;
  }
  return undefined;
}
