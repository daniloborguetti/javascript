function () {
  if ({{user-dlv-customerEmail}}) {
    
    // Se tiver informação no DataLayer executa, senão proximo
    return {{user-dlv-customerEmail}};
  
  } else if ({{user-js-email}}) {
    
    // Se tiver informação no Input do site executa, senão proximo
    return {{user-js-email}};

  } else if ({{cookie-LeadEmail}}) {
    
    // Se tiver informação nos Cookies executa, senão vazio
    return {{cookie-LeadEmail}};

  } else {
    return undefined;
  }
}
