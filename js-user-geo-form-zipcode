function formatarCEP() {
  var name = document.querySelector('[id="postcode"]'); // Substitua pela variável do seu formulário
  var zipcode = name.value;

  // Extrai os primeiros 5 caracteres numéricos
  var zipcodeFormated = zipcode.match(/\d{5}/);

  if (zipcode) {
    return zipcodeFormated[0]; // Retorna os primeiros 5 caracteres numéricos
  } else {
    return ""; // Retorna uma string vazia se não encontrar números
  }
}
